{
    "name": "boulzy/manager-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle providing managers to handle your domain logics using Doctrine.",
    "keywords": ["manager", "bundle", "symfony", "doctrine", "domain", "model", "logic"],
    "homepage": "https://github.com/B0ulzy/BoulzyManagerBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Houdelette",
            "email": "b0ulzy.todo@gmail.com",
            "homepage": "https://github.com/B0ulzy",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Boulzy\\ManagerBundle\\": ""
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Boulzy\\ManagerBundle\\": "Tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^3.3 || ^4.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.6",

        "doctrine/common": "^2.8",
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.2"
    },
    "minimum-stability": "stable"
}
